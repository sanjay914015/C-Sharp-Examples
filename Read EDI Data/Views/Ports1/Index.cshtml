@model IEnumerable<Read_EDI_Data.Models.Ports>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Generate EDI Data</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ISA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ST)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Big)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.N1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.N3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.N4)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ISA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ST)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Big)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.N1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.N3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.N4)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
